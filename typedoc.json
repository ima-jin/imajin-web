{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "lib/services",
    "lib/utils",
    "lib/mappers",
    "lib/auth",
    "lib/contacts"
  ],
  "entryPointStrategy": "expand",
  "out": "docs/api/typedoc-output",
  "json": "docs/api/typedoc-data.json",
  "plugin": [],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/tests/**",
    "**/app/**",
    "**/components/**",
    "**/scripts/**"
  ],
  "readme": "none",
  "cleanOutputDir": true,
  "includeVersion": false,
  "searchInComments": true,
  "sort": ["source-order"],
  "skipErrorChecking": true,
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable"
  ]
}
